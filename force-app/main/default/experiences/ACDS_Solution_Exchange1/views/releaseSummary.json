    }, {
      "componentAttributes" : {
        "richTextValue" : "<table>\n  \t<tr style=\"background: #0089AD;box-shadow: 0px 1px 0px #DDDBDA;color:#FFFFFF;flex: none;order: 0;flex-grow: 0;align-items: center;font-family: 'Roboto';font-style: normal;font-weight: 700;font-size: 14px;line-height: 20px;\">\n        <th style=\"padding:6px 12px;\">Release</th>\n        <th style=\"padding:6px 12px;\">Version</th>\n        <th style=\"padding:6px 12px;\">Date</th>\n        <th style=\"padding:6px 12px;\">Package Installation URL</th>\n        <th style=\"padding:6px 12px;\">Release Notes</th>\n        <th style=\"padding:6px 12px;\">Post-Installation Instructions</th>\n        <th style=\"padding:6px 12px;\">Post-Upgrade Instructions</th>\n    </tr>\n    <tr>\n        <td style=\"padding:6px 12px;\">July 2022</td>\n        <td style=\"padding:6px 12px;\">1.1</td>\n        <td style=\"padding:6px 12px;\">July 26, 2022</td>\n        <td style=\"padding:6px 12px;\"><a href=\"https://login.salesforce.com/packaging/installPackage.apexp?p0=04t8t0000000SLKAA2\" target=\"_blank\">Production Package</a><br>\n        <a href=\"https://test.salesforce.com/packaging/installPackage.apexp?p0=04t8t0000000SLKAA2\" target=\"_blank\">Sandbox Package</a></td>\n        <td style=\"padding:6px 12px;\"><a href=\"/solutionexchange/s/release-note-1-1\">ACDS v1.1 Release Notes</a></td>\n        <td style=\"padding:6px 12px;\">None</td>\n        <td style=\"padding:6px 12px;\">None</td>\n    </tr>\n    <tr>\n        <td style=\"padding:6px 12px;\">October 2022</td>\n        <td style=\"padding:6px 12px;\">2.1</td>\n        <td style=\"padding:6px 12px;\">October 07, 2022</td>\n        <td style=\"padding:6px 12px;\"><a href=\"https://login.salesforce.com/packaging/installPackage.apexp?p0=04t8t0000000SfoAAE\" target=\"_blank\">Production Package</a><br>\n        <a href=\"https://test.salesforce.com/packaging/installPackage.apexp?p0=04t8t0000000SfoAAE\" target=\"_blank\">Sandbox Package</a></td>\n        <td style=\"padding:6px 12px;\