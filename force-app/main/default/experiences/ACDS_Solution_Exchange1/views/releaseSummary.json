    "components" : [ {
      "componentAttributes" : {
        "richTextValue" : "<h2><br></h2><h2><strong style=\"font-size: 36px;\">Design System Release Summary</strong></h2><p><br></p><p><span style=\"font-size: 16px;\">Each Design System release has a unique link for downloading its package, plus Release Notes, installation instructions, and upgrade instructions.</span></p><h2><strong>&nbsp;</strong></h2><p>&nbsp;</p>"
      },
      "componentName" : "forceCommunity:richTextInline",
      "id" : "3ee06d29-39f1-4997-83d4-900c08f588ec",
      "renderPriority" : "NEUTRAL",
      "renditionMap" : { },
      "type" : "component"
    }, {
      "componentAttributes" : {
        "richTextValue" : "<div style=\"padding: 12px 12px 12px 36px;border-left-width: 5px;border-left-style: solid;border-left-color: #2ab27b;line-height: 1.4em;margin-top: 18px;margin-bottom: 18px;position: relative;background-color: rgba(42, 178, 123, 0.08);margin-left: 0px !important;margin-right: 0px !important;\">\n  <h3 class=\"slds-p-bottom_small\" style=\"font-size: 1.2em;\">Note</h3>\n  <p class=\"slds-p-bottom_small\">To install in a sandbox rather than a production org, alter the download URL by changing \"login\" to \"test\". For example:</p>\n  <p class=\"slds-p-bottom_small\">Production URL: <code>https://<span style=\"text-decoration: underline;\">login</span>.salesforce.com/packaging/installPackage.apexp?p0={package-id...} </code>\n  </p>\n  <p class=\"slds-p-bottom_small\">Sandbox URL: <code>https://<span style=\"text-decoration: underline;\">test</span>.salesforce.com/packaging/installPackage.apexp?p0={package-id...} </code>\n  </p>\n</div>"
      },
      "componentName" : "forceCommunity:htmlBlock",
