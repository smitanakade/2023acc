<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Late_Submission__c</fullName>
    <description>Compares Date first submitted with QFR due date to show if the record was a late submission or not</description>
    <externalId>false</externalId>
    <formula>IF(Date_QFR_First_Submitted__c &gt; QFR_Due_Date__c, true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Late Submission</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
