<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR21_CONREL_Check_PercentageOwnedLess1</fullName>
    <active>false</active>
    <errorConditionFormula>AND(!ISBLANK(Percentage_Owned__c),Percentage_Owned__c&lt;0.01)</errorConditionFormula>
    <errorDisplayField>Percentage_Owned__c</errorDisplayField>
    <errorMessage>The Percentage Owner is less than 1%. Please enter a different Percentage Owned for this owner.</errorMessage>
</ValidationRule>
