<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR24_CONREL_Check_Fully_Owned</fullName>
    <active>false</active>
    <errorConditionFormula>AND(TEXT(Relationship__c)=&quot;Fully Owned&quot;, Percentage_Owned__c&lt;1)</errorConditionFormula>
    <errorDisplayField>Percentage_Owned__c</errorDisplayField>
    <errorMessage>Owner cannot be Fully Owned and have Percentage Owned less than 100%. Please enter a different Percentage Owned.</errorMessage>
</ValidationRule>
