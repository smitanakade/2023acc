<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR26_CONREL_Check_PartiallyOwned_Over99</fullName>
    <active>false</active>
    <errorConditionFormula>AND(Percentage_Owned__c&gt;0.99,Percentage_Owned__c&lt;1)</errorConditionFormula>
    <errorDisplayField>Percentage_Owned__c</errorDisplayField>
    <errorMessage>The Percentage Owned cannot be greater than 99%. Please enter a different Percentage Owned for this owner</errorMessage>
</ValidationRule>
