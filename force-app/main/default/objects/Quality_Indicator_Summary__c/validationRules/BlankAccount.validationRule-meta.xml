<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BlankAccount</fullName>
    <active>true</active>
    <description>Lookup to Account should not be blank</description>
    <errorConditionFormula>ISBLANK( PROVIDER_ID__c )</errorConditionFormula>
    <errorDisplayField>PROVIDER_ID__c</errorDisplayField>
    <errorMessage>Account should not be blank</errorMessage>
</ValidationRule>
