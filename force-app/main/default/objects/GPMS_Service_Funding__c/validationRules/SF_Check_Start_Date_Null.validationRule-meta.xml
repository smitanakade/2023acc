<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SF_Check_Start_Date_Null</fullName>
    <active>true</active>
    <description>The Start Date cannot be left blank if the service record exists.</description>
    <errorConditionFormula>AND(NOT(ISBLANK( Service__c)),ISBLANK(Start_Date__c))</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Start Date cannot  be left blank</errorMessage>
</ValidationRule>
