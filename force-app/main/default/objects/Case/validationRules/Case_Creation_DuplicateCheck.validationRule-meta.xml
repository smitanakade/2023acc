<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Creation_DuplicateCheck</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNEW(),Application__r.Case_Record_Exists__c,RecordType.DeveloperName=&quot;Request_for_Reconsideration&quot;)</errorConditionFormula>
    <errorMessage>You cannot add more than one Reconsideration Request to the Application</errorMessage>
</ValidationRule>
