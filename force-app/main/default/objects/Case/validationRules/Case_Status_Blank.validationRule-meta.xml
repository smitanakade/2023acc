<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Status_Blank</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISBLANK(Text(Status)),Text(Status) = &apos;--None--&apos;)</errorConditionFormula>
    <errorDisplayField>Status</errorDisplayField>
    <errorMessage>Please select RFI Status</errorMessage>
</ValidationRule>
