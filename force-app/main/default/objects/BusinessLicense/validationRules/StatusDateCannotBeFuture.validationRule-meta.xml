<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StatusDateCannotBeFuture</fullName>
    <active>true</active>
    <description>Status Date Cannot Be Future Date</description>
    <errorConditionFormula>Status_Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Status_Date__c</errorDisplayField>
    <errorMessage>Status Date cannot be future date</errorMessage>
</ValidationRule>
