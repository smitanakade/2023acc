    <screens>
        <name>WithdrawConsent</name>
        <label>WithdrawConsent</label>
        <locationX>314</locationX>
        <locationY>134</locationY>
        <allowBack>true</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <backButtonLabel>Cancel</backButtonLabel>
        <connector>
            <targetReference>Get_SRHId</targetReference>
        </connector>
        <fields>
            <name>Warning</name>
            <fieldText>&lt;p&gt;&lt;strong&gt;Has this participant withdrawn their consent?&lt;/strong&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>Early</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;font-size: 12px; background-color: rgb(255, 255, 255);&quot;&gt;This interview was to be included in calculations for the next rating update due&amp;nbsp;on &lt;/span&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;{!recordId.Consumer_Experience_Report__r.Quarterly_RP__r.CER_Calc_Date__c}, and as such the following action will be taken:&lt;/span&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;Demographic data will be nullified&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;Participant responses will be set to withdrawn&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;The service&apos;s CER rating will be calculated on {!recordId.Consumer_Experience_Report__r.Quarterly_RP__r.CER_Calc_Date__c}&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;This particpant&apos;s data will &lt;/span&gt;&lt;strong style=&quot;font-size: 12px;&quot;&gt;not be&lt;/strong&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt; included in the calculated rating.&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>RecalcEarly</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <booleanValue>true</booleanValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>IsMostRecentRecord</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>&apos;Extant&apos;</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>Extant</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;This Interview is attached to {!recordId.Consumer_Experience_Report__r.Service__r.Name}&apos;s most recent CER record and as such the following action will be taken:&lt;/span&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;Demographic data will be nullified&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;Participant responses will be set to withdrawn&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;The service&apos;s CER rating and Overall Star Rating will be recalculated for this reporting period&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;This particpant&apos;s data will &lt;/span&gt;&lt;strong style=&quot;font-size: 12px;&quot;&gt;not be&lt;/strong&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt; included in the recalculated rating.&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>IsMostRecentRecord</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>&apos;Extant&apos;</stringValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>RecalcEarly</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <booleanValue>false</booleanValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
