    <description>Correcting Update - No Recalc action to include Consent_Status__c field</description>
    <environments>Default</environments>
    <formulas>
        <name>IsMostRecentRecord</name>
        <dataType>String</dataType>
        <expression>IF( {!recordId.Consumer_Experience_Report__r.Rating_Exemption_Granted__c} = true, &quot;Extant&quot;,&quot;Past&quot;)</expression>
    </formulas>
    <formulas>
        <name>RecalcEarly</name>
        <dataType>Boolean</dataType>
        <expression>IF(TODAY() &lt;= {!recordId.Consumer_Experience_Report__r.Quarterly_RP__r.CER_Calc_Date__c}, true, false)</expression>
    </formulas>
    <formulas>
        <description>Based on the Recalc Window variable, was the Rating recalculated or not</description>
        <name>RecalcOutcome</name>
        <dataType>String</dataType>
        <expression>IF( 
     AND( {!RecalcEarly}
 = false,
{!recordId.Consumer_Experience_Report__r.Rating_Exemption_Granted__c}
 = true), 
          &quot;WAS recalculated&quot;,
IF( 
    AND( {!RecalcEarly}
 = false,
{!recordId.Consumer_Experience_Report__r.Rating_Exemption_Granted__c}
 = false),
&quot;WAS NOT recalculated&quot;, 
IF( {!RecalcEarly}
 = true,
&quot;WILL BE Recalculated&quot;,
&quot;Encountered an ERROR&quot;)
))</expression>
    </formulas>
