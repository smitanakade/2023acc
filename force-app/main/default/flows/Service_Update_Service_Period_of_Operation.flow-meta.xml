    <decisions>
        <name>Check_Status_of_Services</name>
        <label>Check Status of Services</label>
        <locationX>842</locationX>
        <locationY>455</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>ServicesIsOpened</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Service_Record.Status__c</leftValueReference>
