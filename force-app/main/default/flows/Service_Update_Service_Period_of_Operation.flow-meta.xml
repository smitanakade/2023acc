                <targetReference>Get_Contact_Relationship_Record</targetReference>
            </connector>
            <label>ServicesIsClosed</label>
        </rules>
        <rules>
            <name>APServicesIsClosed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Service_Record.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Service Record Closed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Service_Record.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed - Combined</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Contact_Relationship_Record_2</targetReference>
            </connector>
            <label>APServicesIsClosed</label>
        </rules>
