<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Share_with_Portal_User</fullName>
        <accessLevel>Read</accessLevel>
        <label>Share with Portal User</label>
        <sharedTo>
            <allCustomerPortalUsers></allCustomerPortalUsers>
        </sharedTo>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>City__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <criteriaItems>
            <field>Country__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>GPMS_Address_DM_Integration_User</fullName>
        <accessLevel>Edit</accessLevel>
        <label>GPMS Address DM Integration User</label>
        <sharedTo>
            <group>GPMS_Internal_Users</group>
        </sharedTo>
        <sharedFrom>
            <group>DM_Integration_User</group>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>GPMS_Address_Sharing_Rule</fullName>
        <accessLevel>Edit</accessLevel>
        <label>GPMS Address Sharing Rule</label>
        <sharedTo>
            <group>GPMS_Internal_Users</group>
        </sharedTo>
        <sharedFrom>
            <group>GPMS_Internal_Users</group>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
